<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="eqdfewf" stylesheets="@../tech-officer-ui-Components/tech-officer-student-attendance-css.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.techlms.demoitest.controllers.studentControllers.StudentNewMedicalController">
   <children>

      <!-- Attendance Table Section -->
      <VBox stylesheets="@student-attendance.css" VBox.vgrow="ALWAYS">
         <children>
            <TableView fx:id="medicalTable" VBox.vgrow="ALWAYS">
               <columns>
                  <TableColumn fx:id="medicalNo" editable="false" minWidth="150.0" sortable="false" text="Medical Number" />
                  <TableColumn fx:id="studentId" editable="false" minWidth="150.0" sortable="false" text="Student ID" />
                  <TableColumn fx:id="studentName" editable="false" minWidth="150.0" sortable="false" text="Student Name" />
                  <TableColumn fx:id="startDate" editable="false" minWidth="150.0" sortable="false" text="Medical Start Date" />
                  <TableColumn fx:id="endDate" editable="false" minWidth="150.0" sortable="false" text="Medical End Date" />
                  <TableColumn fx:id="medicalResource" editable="false" minWidth="150.0" sortable="false" text="Medical " />
               </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         </children>
      </VBox>
   </children>
</VBox>
