<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="contentContainer" alignment="CENTER" fillWidth="false" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.techlms.demoitest.controllers.lecturerControllers.CourseManagerialController">
   <children>
       <VBox fx:id="lecturer" alignment="CENTER" styleClass="right-side" stylesheets="@./style-sheets/lecturer-home-page.css" VBox.vgrow="NEVER">
           <children>
               <VBox alignment="CENTER" VBox.vgrow="NEVER">
                   <children>
                       <HBox alignment="CENTER" spacing="30.0" VBox.vgrow="NEVER">
                           <children>
                               <VBox alignment="CENTER" onMouseClicked="#switchUndergraduatesPage" styleClass="admin-user-page-card">
                                   <children>
                                       <HBox>
                                           <children>
                                               <VBox>
                                                   <children>
                                                       <ImageView fitHeight="100.0" fitWidth="100.0" pickOnBounds="true" preserveRatio="true">
                                                           <image>
                                                               <Image url="@../../application-images/lecturer-images/779332.png" />
                                                           </image>
                                                       </ImageView>
                                                   </children>
                                               </VBox>
                                               <VBox alignment="CENTER" layoutX="10.0" layoutY="10.0">
                                                   <children>
                                                       <Label styleClass="admin-user-page-card-label" text="Undergraduates" />
                                                   </children>
                                               </VBox>
                                           </children>
                                       </HBox>
                                   </children>
                               </VBox>
                               <VBox alignment="CENTER" layoutX="10.0" layoutY="10.0" onMouseClicked="#switchUserUpdatePage" styleClass="admin-user-page-card">
                                   <children>
                                       <HBox>
                                           <children>
                                               <VBox>
                                                   <children>
                                                       <ImageView fitHeight="100.0" fitWidth="100.0" pickOnBounds="true" preserveRatio="true">
                                                           <image>
                                                               <Image url="@../../application-images/lecturer-images/8386386.png" />
                                                           </image>
                                                       </ImageView>
                                                   </children>
                                               </VBox>
                                               <VBox alignment="CENTER" layoutX="10.0" layoutY="10.0" onMouseClicked="#switchToEligiblity">
                                                   <children>
                                                       <Label styleClass="admin-user-page-card-label" text="Exam Eligibility" />
                                                   </children>
                                               </VBox>
                                           </children>
                                       </HBox>
                                   </children>
                               </VBox>
                               <VBox alignment="CENTER" layoutX="320.0" layoutY="10.0" onMouseClicked="#switchGetAllUsersPage" styleClass="admin-user-page-card">
                                   <children>
                                       <HBox>
                                           <children>
                                               <VBox>
                                                   <children>
                                                       <ImageView fitHeight="100.0" fitWidth="100.0" pickOnBounds="true" preserveRatio="true">
                                                           <image>
                                                               <Image url="@../../application-images/lecturer-images/staff-attendance-01.png" />
                                                           </image>
                                                       </ImageView>
                                                   </children>
                                               </VBox>
                                               <VBox alignment="CENTER" layoutX="10.0" layoutY="10.0" onMouseClicked="#switchtoUndergaduatesMedicalPage">
                                                   <children>
                                                       <Label styleClass="admin-user-page-card-label" text="Undergraduates Medical" />
                                                   </children>
                                               </VBox>
                                           </children>
                                       </HBox>
                                   </children>
                               </VBox>
                               <VBox alignment="CENTER" layoutX="630.0" layoutY="10.0" onMouseClicked="#switchDeletePage" styleClass="admin-user-page-card">
                                   <children>
                                       <HBox>
                                           <children>
                                               <VBox>
                                                   <children>
                                                       <ImageView fitHeight="100.0" fitWidth="100.0" pickOnBounds="true" preserveRatio="true">
                                                           <image>
                                                               <Image url="@../../application-images/lecturer-images/13868555.png" />
                                                           </image>
                                                       </ImageView>
                                                   </children>
                                               </VBox>
                                               <VBox alignment="CENTER" layoutX="10.0" layoutY="10.0">
                                                   <children>
                                                       <Label onMouseClicked="#undergaduatesMarks" styleClass="admin-user-page-card-label" text="Undergraduats Marks" />
                                                   </children>
                                               </VBox>
                                           </children>
                                       </HBox>
                                   </children>
                               </VBox>
                           </children>
                       </HBox>
                   </children>
                   <VBox.margin>
                       <Insets bottom="30.0" left="30.0" right="30.0" top="30.0" />
                   </VBox.margin>
               </VBox>

               <VBox fx:id="userDataCard">
                   <children>
                       <VBox alignment="BOTTOM_CENTER" maxHeight="400.0" VBox.vgrow="NEVER">
                           <children>
                               <HBox alignment="BOTTOM_CENTER" spacing="40.0">
                                   <children>
                                       <VBox alignment="CENTER" styleClass="user-barchart-card">
                                 <children>
                                    <HBox stylesheets="@style-sheets/course-material-table.css">
                                       <children>
                                          <TableView fx:id="courseMaterialTable">
                                            <columns>
                                              <TableColumn fx:id="materialNumber" editable="false" prefWidth="100.0" sortable="false" text="Lecturer No" />
                                              <TableColumn fx:id="lectureTitle" editable="false" prefWidth="250.0" sortable="false" text="Lecturer title" />
                                                <TableColumn fx:id="lectureUploadDate" editable="false" prefWidth="150.0" sortable="false" text="Upload Date" />
                                                <TableColumn fx:id="lectureResource" editable="false" prefWidth="250.0" sortable="false" text="Lecturer  Resources" />
                                            </columns>
                                             <columnResizePolicy>
                                                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                                             </columnResizePolicy>
                                          </TableView>
                                       </children>
                                    </HBox>
                                 </children>
                                       </VBox>
                                       <VBox layoutX="10.0" layoutY="10.0" styleClass="user-barchart-card" HBox.hgrow="ALWAYS">
                                           <children>
                                               <VBox spacing="20.0" VBox.vgrow="ALWAYS">
                                                   <children>
                                          <VBox alignment="CENTER" onMouseClicked="#switchUndergraduatesPage" styleClass="admin-user-page-card">
                                             <children>
                                                <HBox>
                                                   <children>
                                                      <VBox>
                                                         <children>
                                                            <ImageView fitHeight="100.0" fitWidth="100.0" pickOnBounds="true" preserveRatio="true">
                                                               <image>
                                                                  <Image url="@../../application-images/lecturer-images/3658756.png" />
                                                               </image>
                                                            </ImageView>
                                                         </children>
                                                      </VBox>
                                                      <VBox alignment="CENTER" layoutX="10.0" layoutY="10.0">
                                                         <children>
                                                            <Label styleClass="admin-user-page-card-label" text="upload lecture" />
                                                         </children>
                                                      </VBox>
                                                   </children>
                                                </HBox>
                                             </children>
                                          </VBox>
                                          <VBox alignment="CENTER" layoutX="10.0" layoutY="10.0" onMouseClicked="#switchUndergraduatesPage" styleClass="admin-user-page-card">
                                             <children>
                                                <HBox>
                                                   <children>
                                                      <VBox>
                                                         <children>
                                                            <ImageView fitHeight="100.0" fitWidth="100.0" pickOnBounds="true" preserveRatio="true">
                                                               <image>
                                                                  <Image url="@../../application-images/lecturer-images/5580015.png" />
                                                               </image>
                                                            </ImageView>
                                                         </children>
                                                      </VBox>
                                                      <VBox alignment="CENTER" layoutX="10.0" layoutY="10.0">
                                                         <children>
                                                            <Label styleClass="admin-user-page-card-label" text="Update" />
                                                         </children>
                                                      </VBox>
                                                   </children>
                                                </HBox>
                                             </children>
                                          </VBox>
                                          <VBox alignment="CENTER" layoutX="10.0" layoutY="150.0" onMouseClicked="#switchUndergraduatesPage" styleClass="admin-user-page-card">
                                             <children>
                                                <HBox>
                                                   <children>
                                                      <VBox>
                                                         <children>
                                                            <ImageView fitHeight="100.0" fitWidth="100.0" pickOnBounds="true" preserveRatio="true">
                                                               <image>
                                                                  <Image url="@../../application-images/lecturer-images/14620389.png" />
                                                               </image>
                                                            </ImageView>
                                                         </children>
                                                      </VBox>
                                                      <VBox alignment="CENTER" layoutX="10.0" layoutY="10.0">
                                                         <children>
                                                            <Label styleClass="admin-user-page-card-label" text="Delete" />
                                                         </children>
                                                      </VBox>
                                                   </children>
                                                </HBox>
                                             </children>
                                          </VBox>
                                                   </children>
                                               </VBox>
                                           </children>
                                           <HBox.margin>
                                               <Insets />
                                           </HBox.margin>
                                       </VBox>
                                   </children>
                               </HBox>
                           </children>
                           <VBox.margin>
                               <Insets bottom="30.0" left="30.0" right="30.0" top="60.0" />
                           </VBox.margin>
                       </VBox>
                   </children>
               </VBox>
           </children>
           <padding>
               <Insets bottom="10.0" left="10.0" right="50.0" top="20.0" />
           </padding>
       </VBox>

   </children>
</VBox>
